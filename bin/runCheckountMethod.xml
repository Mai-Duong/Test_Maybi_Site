			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="reportConfig.ReportNGListener" />
	</listeners>

	<parameter name="url" value="https://maybi.com/" />


 <!-- Định nghĩa một test -->
    <test name="Run on CHROME">
