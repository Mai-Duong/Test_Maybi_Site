		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="reportConfig.ReportNGListener" />
	</listeners>

	<parameter name="url"
		value="https://maybi.com/" />



	<test name="Run on CHROME">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.maybestore.Testcase_Login" />
			<class name="com.maybestore.Testcase_Register" />
			<class name="com.maybestore.Testcase_Search" />
			<class name="com.maybestore.Testcase_Cart" />
			<class name="com.maybestore.estcase_checkount" />
		</classes>
	</test> <!-- Test -->

